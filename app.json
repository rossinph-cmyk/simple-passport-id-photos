{
  "expo": {
    "name": "Simple Passport ID & Photos",
    "slug": "simple-passport-id-photos",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "simplepassport",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.simplepassportid.app",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos",
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera",
        "NSPhotoLibraryAddUsageDescription": "Allow $(PRODUCT_NAME) to save photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.simplepassportid.app",
      "versionCode": 7,
      "permissions": [
        "CAMERA",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.WRITE_MEDIA_IMAGES"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://simplepassportid.app/"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Simple Passport ID & Photos to access your photos"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Simple Passport ID & Photos to access your camera",
          "recordAudioAndroid": false
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Simple Passport ID & Photos to access your photos.",
          "savePhotosPermission": "Allow Simple Passport ID & Photos to save photos.",
          "isAccessMediaLocationEnabled": false,
          "androidPermissions": [
            "READ_MEDIA_IMAGES",
            "READ_MEDIA_VISUAL_USER_SELECTED"
          ]
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://simplepassportid.app/"
      },
      "eas": {
        "projectId": "d0744897-d760-46cf-88fd-7094a8d05a03"
      }
    },
    "owner": "simplepassport",
    "privacyPolicy": "https://docs.google.com/document/d/e/2PACX-1vSjsY9QbbhEQwk5j7Kp9Q7Q-6lNt_mPqKXQgDZCHj79bI_KO3L1Doq7CgVuVdM6yvG3vFqImYKiUrAC/pub"
  }
}
